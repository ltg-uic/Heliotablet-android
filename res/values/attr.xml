<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TheoryPlanetView">
        <attr name="anchor" format="string"/>
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr name="flag" format="string"/>
        <attr name="textColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="TheoryViewFragment">
        <attr name="android:label"/>
    </declare-styleable>
    <declare-styleable name="ObservationViewFragment">
        <attr name="android:label"/>
    </declare-styleable>
    <declare-styleable name="CircleLayout">
        <attr name="innerRadius" format="dimension"/>
