        <attr name="innerCircle" format="color"/>
        <attr name="angleOffset" format="float"/>
        <attr name="dividerWidth" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="AppTheme">
        <attr name="textAppearanceFinish" format="reference"/>
    </declare-styleable>

</resources>